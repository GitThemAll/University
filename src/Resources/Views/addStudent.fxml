<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<StackPane prefWidth="231.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.AddStudentController">
   <children>
       <AnchorPane minHeight="-Infinity" minWidth="-Infinity" prefHeight="700.0" prefWidth="700.0">
         <children>
            <TextField fx:id="NewStudentUsername" layoutX="267.0" layoutY="113.0" promptText="username" />
            <TextField fx:id="NewStudentBirthdate" layoutX="267.0" layoutY="341.0" promptText="birthdate" />
            <TextField fx:id="NewStudentLastname" layoutX="267.0" layoutY="282.0" promptText="lastname" />
            <TextField fx:id="NewStudentPassword" layoutX="267.0" layoutY="172.0" promptText="password" />
            <TextField fx:id="NewStudentFirstname" layoutX="267.0" layoutY="227.0" promptText="firstname" />
            <Label alignment="CENTER" layoutX="192.0" layoutY="30.0" prefHeight="59.0" prefWidth="281.0" text="Add new Student" textAlignment="CENTER">
               <font>
                  <Font size="26.0" />
               </font></Label>
            <Button fx:id="NewStudentAddStudent" layoutX="473.0" layoutY="453.0" mnemonicParsing="false" onMouseClicked="#AddNewStudent" prefHeight="59.0" prefWidth="160.0" text="Add Student" />
